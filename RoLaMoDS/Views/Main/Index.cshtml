@model System.ValueTuple<string[,],string>

<div id="PageEnteringForm" class="TargetPage CenteredPage">
        <a class="NonUnderline CenteredContainer Container300x200" href="#ImageFromLocal">
            Выбрать изображение
        </a>
</div>

<div class="FullPage">
     <div id = "ImageFromEthernet" class="TargetPage ContentTab">
        <form>
                        <div class="LineContent">

                 <div class="Form-Input">
                    <input class="Form-Field" name="Latitude" type="number" min="-90" max="90" placeholder="Широта">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Широта</label>
                </div>
                  <div class="Form-Input">
                    <input class="Form-Field" name="Longitude"  type="number" min="-180" max="180" placeholder="Долгота">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Долгота</label>
                </div>
                <div class="Form-Input">
                    <input class="Form-Field" name="Scale" type="number" min="5" max="50"  placeholder="Масштаб">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Масштаб</label>
                </div>
                
            </div>
            <div class="LineContent">
                <div class="Form-Input">
                        <input class="Form-Field" name="URL" type="text" placeholder="Image URL">
                        <div class="Form-Input-Bar"></div>
                        <label class="Form-Label">Image URL</label>
                </div>
               <div>
                    <input class="Form-Button" type="submit" value="Загрузить" />
                </div>
            </div>
        </form>
     </div>
    <div id = "ImageFromGoogleMaps" class="TargetPage ContentTab">
             <div class="LineContent">

                 <div class="Form-Input">
                    <input class="Form-Field" name="Latitude" type="number" min="-90" max="90" placeholder="Широта">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Широта</label>
                </div>
                  <div class="Form-Input">
                    <input class="Form-Field" name="Longitude"  type="number" min="-180" max="180" placeholder="Долгота">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Долгота</label>
                </div>
                <div class="Form-Input">
                    <input class="Form-Field" name="Scale" type="number" min="5" max="50"  placeholder="Масштаб">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Масштаб</label>
                </div>
            </div>
            <div>
                <input class="Form-Button" type="submit" value="Загрузить" />
            </div>
     </div>
    <div id = "ImageFromLocal" class="TargetPage ContentTab">
        <form>
            <div class="LineContent">

                 <div class="Form-Input">
                    <input class="Form-Field" name="Latitude" type="number" min="-90" max="90" placeholder="Широта">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Широта</label>
                </div>
                  <div class="Form-Input">
                    <input class="Form-Field" name="Longitude"  type="number" min="-180" max="180" placeholder="Долгота">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Долгота</label>
                </div>
                <div class="Form-Input">
                    <input class="Form-Field" name="Scale" type="number" min="5" max="50"  placeholder="Масштаб">
                    <div class="Form-Input-Bar"></div>
                    <label class="Form-Label">Масштаб</label>
                </div>
                
            </div>
            <div class="LineContent">
                <div>
                
                    <input id ="Image-Upload-Input" class="Form-Field" type="file" name="File" 
                     accept="image/*"/>
                    <label for="Image-Upload-Input">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 17">
                            <path d="M10 0l-5.2 4.9h3.3v5.1h3.8v-5.1h3.3l-5.2-4.9zm9.3 11.5l-3.2-2.1h-2l3.4 2.6h-3.5c-.1 0-.2.1-.2.1l-.8 2.3h-6l-.8-2.2c-.1-.1-.1-.2-.2-.2h-3.6l3.4-2.6h-2l-3.2 2.1c-.4.3-.7 1-.6 1.5l.6 3.1c.1.5.7.9 1.2.9h16.3c.6 0 1.1-.4 1.3-.9l.6-3.1c.1-.5-.2-1.2-.7-1.5z">
                            </path>
                        </svg>
                        <span>Выбрать файл изображения</span>
                </label>
                    
                </div>
                <div>
                    <input class="Form-Button" type="submit" value="Загрузить" />
                </div>
            </div>
        </form>
    </div>
    <div class="LeftTabs">
            <a href="#ImageFromLocal" class="NonUnderline White"><span>From local</span></a>
            <a href="#ImageFromEthernet" class="NonUnderline White"><span> From Ethernet</span></a>
            <a href="#ImageFromGoogleMaps" class="NonUnderline White"><span> From Google Maps</span></a>
    </div>
    <div id="ImageList" class="RightBottomPage">
        <img class="ActiveImage" src="https://images6.alphacoders.com/702/702757.png" />
        
        <button id="ImagePrevLeft" ><</button>
        <button id="ImageNextRight" >></button>
        <button id="RecognizeImage"class="Form-Button">Распознать</button>
    </div>
</div>
